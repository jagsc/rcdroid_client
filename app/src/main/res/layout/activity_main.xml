<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
											 xmlns:app="http://schemas.android.com/apk/res-auto"
											 xmlns:tools="http://schemas.android.com/tools"
											 android:layout_width="match_parent"
											 android:layout_height="match_parent"
											 android:visibility="visible"
											 tools:context="jp.android_group.student.abc2017a_client.MainActivity">

	<SeekBar
		android:id="@+id/SpeedBar"
		android:layout_width="368dp"
		android:layout_height="13dp"
		android:layout_alignParentEnd="true"
		android:layout_alignParentTop="true"
		android:layout_marginTop="80dp"
		android:visibility="visible"
		app:layout_constraintTop_toBottomOf="@+id/BackButton"
		tools:layout_constraintTop_creator="1"
		android:layout_marginLeft="8dp"
		app:layout_constraintLeft_toLeftOf="parent"
		android:layout_marginRight="8dp"
		app:layout_constraintRight_toRightOf="parent" />

	<Button
		android:id="@+id/BackButton"
		android:layout_width="0dp"
		android:layout_height="61dp"
		android:layout_alignEnd="@+id/AccelButton"
		android:layout_below="@+id/AccelButton"
		android:layout_column="1"
		android:layout_marginTop="44dp"
		android:layout_row="2"
		android:layout_weight="1"
		android:elevation="0dp"
		android:text="@string/Bre"
		app:layout_constraintHorizontal_bias="0.535"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/AccelButton"
		tools:layout_constraintLeft_creator="1"
		tools:layout_constraintRight_creator="1"
		tools:layout_constraintTop_creator="1" />

	<Button
		android:id="@+id/AccelButton"
		android:layout_width="0dp"
		android:layout_height="64dp"
		android:layout_below="@+id/SpeedBar"
		android:layout_centerHorizontal="true"
		android:layout_column="1"
		android:layout_row="1"
		android:layout_weight="1"
		android:text="@string/Acc"
		tools:layout_constraintTop_creator="1"
		tools:layout_constraintRight_creator="1"
		app:layout_constraintRight_toRightOf="parent"
		android:layout_marginTop="100dp"
		app:layout_constraintTop_toBottomOf="@+id/LeftButton"
		tools:layout_constraintLeft_creator="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintHorizontal_bias="0.0" />

	<ToggleButton
		android:id="@+id/LeftButton"
		android:layout_width="180dp"
		android:layout_height="58dp"
		android:layout_alignBaseline="@+id/RightButton"
		android:layout_alignBottom="@+id/RightButton"
		android:layout_alignParentStart="true"
		android:layout_weight="1"
		android:checked="false"
		android:text="@string/Left"
		android:textOff="@string/Left"
		android:textOn="@string/Left"
		android:layout_marginLeft="8dp"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="100dp" />

	<ToggleButton
		android:id="@+id/RightButton"
		android:layout_width="180dp"
		android:layout_height="56dp"
		android:layout_above="@+id/SpeedBar"
		android:layout_alignParentEnd="true"
		android:layout_marginRight="8dp"
		android:layout_marginTop="100dp"
		android:layout_weight="1"
		android:checked="false"
		android:text="@string/Right"
		android:textOff="@string/Right"
		android:textOn="@string/Right"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="200dp" />

</android.support.constraint.ConstraintLayout>